{
    "name": "renoki-co/laravel-web3",
    "description": "Laravel SDK wrapper for the Web3 PHP API client that interacts with the Ethereum blockchain.",
    "keywords": [
        "laravel",
        "php",
        "api",
        "sdk",
        "wrapper",
        "web3",
        "eth",
        "ethereum",
        "blockchain",
        "token",
        "contracts"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-web3",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "web3-php/web3": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelWeb3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelWeb3\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.3",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelWeb3\\LaravelWeb3ServiceProvider"
            ]
        }
    }
}
